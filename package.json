{
  "name": "korres",
  "version": "1.0.2",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev -- --force",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "server:start": "ts-node server/index.ts"
  },
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@googleapis/gmail": "^12.0.0",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-router": "^1.56.5",
    "@types/kakao-js-sdk": "^1.39.5",
    "antd": "^5.17.0",
    "axios": "^1.7.7",
    "dayjs": "^1.11.11",
    "electron-updater": "^6.1.7",
    "fastify": "^4.28.1",
    "immer": "^10.1.1",
    "korail-ts": "file:../korail-ts",
    "lodash": "^4.17.21",
    "nodemailer": "^6.9.15",
    "qs": "^6.12.1",
    "usehooks-ts": "^3.1.0",
    "vercel": "^37.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/register": "^7.23.7",
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^2.4.0",
    "@tanstack/eslint-plugin-query": "^5.66.0",
    "@tanstack/router-devtools": "^1.56.5",
    "@tanstack/router-plugin": "^1.57.1",
    "@types/lodash": "^4.17.1",
    "@types/node": "^18.19.9",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
